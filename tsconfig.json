{
  "compilerOptions": {
    "target": "ES2016",
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./dist",
    "baseUrl": "./src",
    "paths": {
      "@api/*": ["api/*"],
      "@core/*": ["core/*"],
      "@config/*": ["config/*"],
      "@databases/*": ["databases/*"],
      "@modules/*": ["modules/*"],
      "@server/*": ["server/*"],
      "@config": ["config/index"],
    },
    "esModuleInterop": true,
    "strictPropertyInitialization": false,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
  },
}